




def main():
      cleaned_shows = []
      for show in SHOWS:
        print(show.capitalize())     #Only uppercase first letter
        print(show.title())          #Uppercase first letter of every word
        print(show.strip())          #Strips leading and trailing spaces
        cleaned_shows.append(show.strip().title())

      print(', '.join(cleaned_shows))    #Prints list of shows with comma and space "clean print"

main()
